<h1>Cookie Struggle? You Are Not Alone.<h1>


<%= form_for :question, url: questions_path do |f| %>

  <p>
    <%= f.text_field :title, :placeholder => "Title" %>
  </p>

  <p>
    <%= f.text_area :description, :placeholder => "Your Question Here" %>
  </p>

  <p>
    <%= f.submit "Create Question" %>
  </p>
<% end %>
